@model IEnumerable<mte.Models.WorkTypes>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsNullRun)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsWork)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsDinner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsBreak)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsNullRun)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsWork)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsDinner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsBreak)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
