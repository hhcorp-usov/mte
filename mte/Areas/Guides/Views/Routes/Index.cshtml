}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Enterprises.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PointStart.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PointStop.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RouteTypes.Name)
        </th>
        <th>
