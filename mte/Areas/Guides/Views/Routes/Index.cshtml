        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PointStop.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RouteTypes.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BackName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
