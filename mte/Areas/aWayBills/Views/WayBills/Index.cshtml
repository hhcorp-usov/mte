@model IEnumerable<mte.Models.WayBills>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cars.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enterprises.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WayBillStatuses.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateAdd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateClose)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cars.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enterprises.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WayBillStatuses.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateAdd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateClose)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id }) |
            @Html.ActionLink("Print", "Print", new { id = item.Id })
        </td>
    </tr>
}

</table>
